      {
        "exported": true,
        "srcEntrance": "./ets/ServiceExtAbility/ExtAbility.ts",
        "name": "com.ohos.systemui.dialog",
        "description": "$string:mainability_description",
        "type": "service",
        "permissions": [
          "ohos.permission.START_SYSTEM_DIALOG"
        ]
      }
    ]
  }
}